{
    "id": "wild-animals-fdg",
    "name": "Wild Animals FDG",
    "description": "Argumentation framework of wild animals (attacks network)",
    "domainLanguageLines": [
        "@Recursive(Defeated, iterative: true);",
        "",
        "Attacks(\"B\",\"A\");",
        "Attacks(\"C\",\"B\");",
        "Attacks(\"E\",\"B\");",
        "Attacks(\"F\",\"B\");",
        "Attacks(\"V\",\"B\");",
        "Attacks(\"W\",\"B\");",
        "Attacks(\"Y\",\"B\");",
        "Attacks(\"D\",\"C\");",
        "Attacks(\"Q\",\"C\");",
        "Attacks(\"Z\",\"C\");",
        "Attacks(\"E\",\"D\");",
        "Attacks(\"X\",\"D\");",
        "Attacks(\"M\",\"E\");",
        "Attacks(\"I\",\"E\");",
        "Attacks(\"S\",\"E\");",
        "Attacks(\"H\",\"F\");",
        "Attacks(\"S\",\"F\");",
        "Attacks(\"Q\",\"G\");",
        "Attacks(\"Q\",\"H\");",
        "Attacks(\"J\",\"I\");",
        "Attacks(\"K\",\"I\");",
        "Attacks(\"L\",\"I\");",
        "Attacks(\"O\",\"I\");",
        "Attacks(\"N\",\"J\");",
        "Attacks(\"O\",\"M\");",
        "Attacks(\"M\",\"N\");",
        "Attacks(\"M\",\"O\");",
        "Attacks(\"G\",\"S\");",
        "Attacks(\"T\",\"S\");",
        "Attacks(\"B\",\"T\");",
        "Attacks(\"U\",\"T\");",
        "Attacks(\"V\",\"U\");",
        "",
        "Argument(x) distinct:- Attacks(x,y)| Attacks(y,x);",
        "",
        "Defeated(x) :- Attacks(y,x), Accepted(y);",
        "Accepted(x) :- Argument(x), ~(Attacks(y,x), ~Defeated(y));",
        "Drawn(x) :- Argument(x), ~Defeated(x), ~Accepted(x);",
        "",
        "Result(x, \"defeated\") distinct :- Defeated(x);",
        "Result(x, \"accepted\") distinct :- Accepted(x);",
        "Result(x, \"drawn\") distinct :- Drawn(x);"
    ],
    "visualLanguageLines": [
        "# Graph properties",
        "Graph( id: \"af\", rankdir: \"BT\", engine: \"fdp\");",
        "",
        "# Node definitions - map domain arguments to visual nodes",
        "Node( node_id: x, label: x, shape: \"circle\", style: \"filled\", color:\"#40cfff\", fontsize: \"14\") :- Result(x, \"accepted\");",
        "",
        "Node( node_id: x, label: x, shape: \"circle\", style: \"filled\", color:\"#ffb763\", fontsize: \"14\") :- Result(x, \"defeated\");",
        "",
        "Node( node_id: x, label: x, shape: \"circle\", style: \"filled\", color:\"#FEFE62\", fontsize: \"14\") :- Result(x, \"drawn\");",
        "",
        "# Edge definitions - map domain attacks to visual edges",
        "Edge(source_id: source, target_id: target, color: \"black\", style: \"solid\", arrowhead: \"normal\", arrowtail: \"\") :- Attacks(source, target);"
    ]
}